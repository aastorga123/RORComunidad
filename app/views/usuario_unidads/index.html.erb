<p id="notice"><%= notice %></p>

<h1>Listing Usuario Unidads</h1>

<table>
  <thead>
    <tr>
      <th>Usuario</th>
      <th>Unidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuario_unidads.each do |usuario_unidad| %>
      <tr>
        <td><%= usuario_unidad.usuario %></td>
        <td><%= usuario_unidad.unidad %></td>
        <td><%= link_to 'Show', usuario_unidad %></td>
        <td><%= link_to 'Edit', edit_usuario_unidad_path(usuario_unidad) %></td>
        <td><%= link_to 'Destroy', usuario_unidad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario unidad', new_usuario_unidad_path %>
