<p id="notice"><%= notice %></p>

<h1>Lista Tipo Cuenta</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_cuenta.each do |tipo_cuentum| %>
      <tr>
        <td><%= tipo_cuentum.tipo %></td>
        <td><%= link_to 'Show', tipo_cuentum %></td>
        <td><%= link_to 'Edit', edit_tipo_cuentum_path(tipo_cuentum) %></td>
        <td><%= link_to 'Destroy', tipo_cuentum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo cuentum', new_tipo_cuentum_path %>
